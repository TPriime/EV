<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.effect.InnerShadow?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>

<StackPane maxHeight="-Infinity" minWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="electionTitle" text="election title" StackPane.alignment="CENTER_LEFT" />
      <HBox alignment="CENTER_RIGHT" spacing="10.0">
         <children>
            <Label fx:id="partyName" text="party name" />
            <ImageView fx:id="partyLogo" fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" HBox.hgrow="NEVER">
               <viewport>
                  <Rectangle2D />
               </viewport></ImageView>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="25.0" right="25.0" top="10.0" />
   </padding>
   <effect>
      <InnerShadow />
   </effect>
</StackPane>
